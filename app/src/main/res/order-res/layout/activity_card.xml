<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/png_20220319_122309_00010"
    tools:context=".Card">

    <EditText
        android:id="@+id/editTextTotal2"
        android:layout_width="159dp"
        android:layout_height="39dp"
        android:layout_marginEnd="84dp"
        android:ems="10"
        android:hint="@string/editTextTotal"
        android:inputType="number"
        android:text="Rs."
        app:layout_constraintBaseline_toBaselineOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="412dp"
        android:layout_height="53dp"
        android:layout_marginBottom="4dp"
        android:background="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Total"
        app:layout_constraintVertical_bias="0.957" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginBottom="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView7"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/png_1" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="36dp"
        android:layout_height="44dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView8"
        app:srcCompat="@drawable/png_2" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="40dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView10"
        app:srcCompat="@drawable/png_3"
        tools:layout_editor_absoluteY="679dp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="38dp"
        android:layout_height="42dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/png_5" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="48dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView9"
        app:srcCompat="@drawable/png_4"
        tools:layout_editor_absoluteY="677dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="73dp"
        android:layout_height="25dp"
        android:background="#FFD12A"
        android:hint="@string/buy"
        android:text="Buy Now"
        tools:ignore="TouchTargetSizeCheck"
        tools:layout_editor_absoluteX="313dp"
        tools:layout_editor_absoluteY="514dp" />

    <Button
        android:id="@+id/button12"
        android:layout_width="164dp"
        android:layout_height="28dp"
        android:layout_marginTop="176dp"
        android:layout_marginEnd="124dp"
        android:background="#FFD12A"
        android:hint="@string/buy"
        android:text="Place Order"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/price_dis2"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/button5"
        android:layout_width="73dp"
        android:layout_height="25dp"
        android:background="#FFFFFF"
        android:hint="@string/Remove"
        android:text="Remove"
        tools:ignore="TouchTargetSizeCheck"
        tools:layout_editor_absoluteX="240dp"
        tools:layout_editor_absoluteY="514dp" />

    <Button
        android:id="@+id/remove2"
        android:layout_width="73dp"
        android:layout_height="25dp"
        android:background="#0ED817"
        android:hint="@string/Remove"
        android:text="Update"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="TouchTargetSizeCheck"
        tools:layout_editor_absoluteY="514dp" />

    <TextView
        android:id="@+id/price_dis2"
        android:layout_width="52dp"
        android:layout_height="25dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="12dp"
        android:hint="@string/price_dis"
        android:text="600/-"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextPersonName2"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/prices3"
        android:layout_width="52dp"
        android:layout_height="25dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="20dp"
        android:hint="@string/price"
        android:text="Price"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/prices4"
        app:layout_constraintEnd_toStartOf="@+id/price_dis2" />

    <TextView
        android:id="@+id/Dolpin_kottu"
        android:layout_width="113dp"
        android:layout_height="24dp"
        android:layout_marginBottom="12dp"
        android:hint="@string/Dolpin_kottu"
        android:text="Dolpin kottu"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/prices3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.785"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="53dp"
        android:layout_height="29dp"
        android:ems="10"
        android:hint="@string/editTextNumbe1"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.114"
        app:layout_constraintStart_toEndOf="@+id/prices4"
        tools:ignore="SpeakableTextPresentCheck"
        tools:layout_editor_absoluteY="461dp" />

    <TextView
        android:id="@+id/prices4"
        android:layout_width="54dp"
        android:layout_height="23dp"
        android:layout_marginStart="232dp"
        android:layout_marginBottom="24dp"
        android:hint="@string/price"
        android:text="Qty"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/button6"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/buy_now"
        android:layout_width="73dp"
        android:layout_height="25dp"
        android:background="#FFD12A"
        android:hint="@string/buy"
        android:text="Buy Now"
        tools:ignore="TouchTargetSizeCheck"
        tools:layout_editor_absoluteX="301dp"
        tools:layout_editor_absoluteY="315dp" />

    <Button
        android:id="@+id/Update"
        android:layout_width="73dp"
        android:layout_height="25dp"
        android:layout_marginBottom="48dp"
        android:background="#0ED817"
        android:hint="@string/Update"
        android:text="Update"
        app:layout_constraintBottom_toTopOf="@+id/Dolpin_kottu"
        app:layout_constraintEnd_toStartOf="@+id/Remove"
        app:layout_constraintHorizontal_bias="0.993"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="54dp"
        android:layout_height="33dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="20dp"
        android:ems="10"
        android:hint="@string/editTextNumbe1"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/buy_now"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/Cheese_kottu"
        android:layout_width="113dp"
        android:layout_height="24dp"
        android:layout_marginEnd="72dp"
        android:layout_marginBottom="20dp"
        android:hint="@string/Chicke_kottu"
        android:text="Chicken kottu"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/prices"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/prices"
        android:layout_width="52dp"
        android:layout_height="25dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="20dp"
        android:hint="@string/price"
        android:text="Price"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/prices2"
        app:layout_constraintEnd_toStartOf="@+id/price_dis1" />

    <TextView
        android:id="@+id/price_dis1"
        android:layout_width="53dp"
        android:layout_height="25dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="68dp"
        android:hint="@string/price_dis"
        android:text="600/-"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/buy_now"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/prices2"
        android:layout_width="54dp"
        android:layout_height="23dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="20dp"
        android:hint="@string/price"
        android:text="Qty"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/buy_now"
        app:layout_constraintEnd_toStartOf="@+id/editTextTextPersonName" />

    <Button
        android:id="@+id/Remove"
        android:layout_width="73dp"
        android:layout_height="25dp"
        android:background="#C50303"
        android:hint="@string/Remove"
        android:text="Remove"
        app:layout_constraintBottom_toTopOf="@+id/Dolpin_kottu"
        app:layout_constraintTop_toBottomOf="@+id/prices2"
        app:layout_constraintVertical_bias="0.289"
        tools:ignore="TouchTargetSizeCheck"
        tools:layout_editor_absoluteX="228dp" />

    <TextView
        android:id="@+id/CARD"
        android:layout_width="292dp"
        android:layout_height="62dp"
        android:layout_marginTop="88dp"
        android:fontFamily="@font/abril_fatface"
        android:hint="@string/CARD"
        android:text="Koththuwa Cart"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="32sp"
        app:layout_constraintBottom_toTopOf="@+id/Cheese_kottu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="65dp"
        android:layout_height="26dp"
        android:layout_marginBottom="16dp"
        android:text="Total"
        android:textColorHint="#090909"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/button12"
        app:layout_constraintEnd_toStartOf="@+id/editTextTotal2"
        app:layout_constraintHorizontal_bias="0.758"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>